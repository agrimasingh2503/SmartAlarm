{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "airbnb/hooks"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": 0,
        "object-curly-newline": 0,
        "one-var-declaration-per-line": [2, "initializations"],
        "radix": [2, "as-needed"],
        "no-multi-assign": 0,
        "no-unused-expressions": 0,
        "no-unneeded-ternary": 0,
        "no-confusing-arrow": 0,
        "key-spacing": 2,
        "no-return-assign": 0,
        "function-paren-newline": 0,
        "arrow-body-style": [1, "as-needed"],
        "arrow-parens": [1, "as-needed"],
        "brace-style": 0,
        "camelcase": "warn",
        "class-methods-use-this": 0,
        "comma-dangle": 0,
        "consistent-return": 0,
        "eol-last": 0,
        "func-names": 0,
        "global-require": 0,
        "implicit-arrow-linebreak": 0,
        "import/extensions": 0,
        "import/imports-first": 0,
        "import/no-cycle": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "import/no-useless-path-segments": 1,
        "import/named": 1,
        "indent": ["warn", 4, { "SwitchCase": 1 }],
        "jsx-a11y/alt-text": 0,
        "jsx-a11y/click-events-have-key-events": 0,
        "jsx-a11y/href-no-hash": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "jsx-quotes": 1,
        "keyword-spacing": 1,
        "linebreak-style": 0,
        "lines-between-class-members": 0,
        "max-classes-per-file": 0,
        "max-len": 0,
        "new-cap": 0,
        "no-alert": 0,
        "no-console": 0,
        "no-debugger": 1,
        "no-else-return": 0,
        "no-global-assign": 0,
        "no-lone-blocks": 0,
        "no-multi-spaces": 1,
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
        "no-shadow": 0,
        "no-tabs": 1,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 0,
        "no-unsafe-negation": 0,
        "no-unused-vars": [2, { "varsIgnorePattern": "(bindActionCreators|colors|chalk|dotenv|PropTypes)", "args": "none" }],
        "no-use-before-define": 0,
        "no-useless-constructor": 0,
        "no-var": 1,
        "no-mixed-operators": [1, {"allowSamePrecedence": true}],
        "no-multiple-empty-lines": 0,
        "no-undef": 1,
        "no-useless-return": 0,
        "object-curly-spacing": 2,
        "one-var": 0,
        "operator-linebreak": 0,
        "padded-blocks": 0,
        "prefer-destructuring": 0,
        "quotes": [2, "single"],
        "quote-props": "warn",
        "react-native/no-color-literals": 0,
        "react-native/no-inline-styles": 0,
        "react-native/no-unused-styles": 1,
        "react-native/split-platform-components": 2,
        "react/button-has-type": 0,
        "react/destructuring-assignment": 0,
        "react/display-name": [ 1, { "ignoreTranspilerName": false }],
        "react/forbid-prop-types": [2, { "forbid": ["any"] }],
        "react/jsx-boolean-value": [1, "always"],
        "react/jsx-closing-bracket-location": [1, { "selfClosing": "tag-aligned", "nonEmpty": "after-props" }],
        "react/jsx-curly-spacing": 2,
        "react/jsx-curly-newline": 0,
        "react/jsx-fragments": 0,
        "react/jsx-indent": [1, 4],
        "react/jsx-indent-props": 0,
        "react/jsx-key": 1,
        "react/jsx-max-props-per-line": [1, { "maximum": 5 }],
        "react/jsx-no-bind": 0,
        "react/jsx-no-duplicate-props": 1,
        "react/jsx-no-literals": 0,
        "react/jsx-no-target-blank": 0,
        "react/jsx-no-undef": 1,
        "react/jsx-pascal-case": 1,
        "react/jsx-props-no-spreading": 0,
        "react/jsx-one-expression-per-line": 0,
        "react/jsx-sort-prop-types": 0,
        "react/jsx-sort-props": 0,
        "react/jsx-tag-spacing": [1, {"beforeSelfClosing": "always"}],
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/jsx-wrap-multilines": 1,
        "react/no-access-state-in-setstate": 0,
        "react/no-danger": 1,
        "react/no-did-mount-set-state": 0,
        "react/no-did-update-set-state": 2,
        "react/no-direct-mutation-state": 2,
        "react/no-multi-comp": 0,
        "react/no-set-state": 0,
        "react/no-unknown-property": 1,
        "react/no-unused-prop-types": 1,
        "react/prefer-es6-class": 1,
        "react/prefer-stateless-function": 0,
        "react/react-in-jsx-scope": 1,
        "react/require-extension": 0,
        "react/self-closing-comp": 1,
        "react/sort-comp": 1,
        "react/state-in-constructor": 0,
        "react/static-property-placement": 0,
        "react/require-default-props": 2,
        "semi": [2, "always"],
        "space-infix-ops": "off",
        "spaced-comment": 0,
        "semi-spacing": "off",
        "import/prefer-default-export": 0,
        "react/no-unescaped-entities": "off"
    }
}
